<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.sinau.dao.MemberDao">

	<select id="getPwd" parameterType="String"
		resultType="String">
		select m_pwd from m	where m_email=#{email}
	</select>
	<select id="getMemInfo" parameterType="String"
		resultType="com.sinau.dto.Member">
		select * from minfo where m_email=#{email}
	</select>
	<insert id="MemberInsert" parameterType="com.sinau.dto.Member">
		insert into member
		values(#{m_email},#{m_name},#{m_pwd},#{m_phone},#{m_birth},#{m_license})
	</insert>














</mapper>