<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sinau.dao.ServiceCenterDao">
<select id="getList" parameterType="HashMap"
		resultType="com.sinau.dto.QuestionDto">
		<![CDATA[
		select replace(q_code,'q_','') as q_code, q_title, q_date, m_name from questioninfo
		]]>
	</select>
	<select id="getBoardCount" resultType="Integer">
		SELECT COUNT(*) FROM Questioninfo
	</select>

</mapper>