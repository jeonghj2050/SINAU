  CREATE OR REPLACE FORCE VIEW "PRODORDERS" ("P_CODE", "ORD_CODE", "P_TITLE", "P_PRICE", "ORD_DATE", "ORD_AMOUNT", "ORD_STATE", "ORD_M_EMAIL") AS 
