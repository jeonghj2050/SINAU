  CREATE OR REPLACE FORCE VIEW "P_PAY_INFO" ("P_CODE", "P_TITLE", "M_NAME", "CTS_CODE", "P_PRICE", "F_SYSNAME") AS 
  select 
p.p_code,
p.p_title,
